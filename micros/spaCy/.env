# spaCy NLP Service Configuration

# Service Settings
SERVICE_PORT=5084
SERVICE_VERSION=1.0.0

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# spaCy Configuration
SPACY_MODEL=en_core_web_sm
BATCH_SIZE=10

# Feature Flags
ENABLE_NER=true
ENABLE_CLASSIFICATION=true
ENABLE_KEY_PHRASES=true
ENABLE_EMBEDDINGS=true
ENABLE_LANGUAGE_DETECTION=true

# API Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5082

# Health Check
HEALTH_CHECK_INTERVAL=30